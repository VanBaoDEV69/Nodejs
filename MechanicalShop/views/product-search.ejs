                    <% if (products.length === 0) { %>
                      <h6 style="margin-left: 17px;"><%= message %>: '<%= searchQuery %>'</h6>
                    <% } else { %>
                        <% products.forEach(function(product) { %>
                        <div class="col-sm-6 col-lg-4 col-xl-3">
